class Solution {
    public int solution(int order) {
        int answer = 0;
         String[] arr = Integer.toString(order).split("");
	        
	        for( int i=0;i<arr.length;i++) {
	        	int tmp = Integer.parseInt(arr[i]);
	        	if(tmp ==3 || tmp ==6 || tmp ==9) {
	        		answer++;
	        	}
	        }
	        
	        
        return answer;
    }
}
