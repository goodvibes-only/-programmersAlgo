class Solution {
    public int solution(int i, int j, int k) {
        int answer = 0;
          
	        for(int a = i ; a<j+1;a++) {
	        	String[] arr = Integer.toString(a).split("");
	        	for( int b = 0;b<arr.length;b++) {
	        		if(Integer.parseInt(arr[b])==k) {
	        			answer++;
	        		}
	        	}
	        }
        return answer;
    }
}
