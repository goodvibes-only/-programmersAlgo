class Solution {
    public int[] solution(int n) {
        int[] answer = {};
       	        int cnt =  0;
	        for(int i=1;i<n+1;i++) {
	        	if(n%i==0) {
	        		cnt++;
	        	}
	        }
	        answer = new int[cnt];
	        int cnt2 = 0;
	        for(int i=1;i<n+1;i++) {
	        	if(n%i==0) {
	        		answer[cnt2] = i;
	        		cnt2++;
	        	}
	        }
        
        
        return answer;
    }
}
