class Solution {
    public String solution(String my_string) {
        String answer = "";
         for(char a : my_string.toCharArray()) {
	        	if(Character.isUpperCase(a)) {
	        		answer += Character.toLowerCase(a);
	        	}else if(Character.isLowerCase(a)) {
	        		answer +=Character.toUpperCase(a);
	        	}
	        	
	        }
        return answer;
    }
}


