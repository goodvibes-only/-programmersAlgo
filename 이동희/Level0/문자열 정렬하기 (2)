import java.util.Arrays;
class Solution {
    public String solution(String my_string) {
 
	        
	      String answer = "";
        String[] aa = my_string.toLowerCase().split("");
        
        Arrays.sort(aa);
        
        for(int i = 0; i<aa.length; i++){
            answer += aa[i];   
        }        
        return answer;
    }
}
