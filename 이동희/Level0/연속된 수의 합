class Solution {
    public int[] solution(int num, int total) {
        
        
        int a = 0;
        
        for(int i=1;i<num;i++){
            a = a+i;
        }
        
        int b = total - a;
            
        int n = b / num;
        
        int[] answer = new int[num];
        for( int i=0;i<answer.length;i++){
            answer[i] = n +i;
        }
        
        
        
        return answer;
    }
}
